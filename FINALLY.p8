-- shooting

function shoot()
	new_bullet={
	x= player.x*8,
	y= player.y*8,
	speed=4,
	direction=player.sprite
	}
	sfx(2)
	add(bullet,new_bullet)
	
end

function draw_bullet()
	for b in all(bullet) do
		if b.direction==33 then
			spr(52,b.x,b.y)
		end
	 if b.direction==32 then
			spr(54,b.x,b.y)
		end
		if b.direction==34 then
			spr(55,b.x,b.y)
		end
		if b.direction==49 then
			spr(56,b.x,b.y)
		end
	end
end

function update_bullet()
	for b in all(bullet) do
		if b.direction==33 then
			b.x+=1	
		end
		if b.direction==32 then
			b.x-=1
		end
		if b.direction==34 then
			b.y+=1
		end
		if b.direction==49 then
			b.y-=1
		end
	end
end


