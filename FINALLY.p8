-- zombies

-- dessin zombies
function draw_zombies()
 	for z in all(zombies) do 
  	spr(35,z.x,z.y) 
  end
end

function kill_zemmour()
	for z in all(zombies) do
		for b in all(bullet) do
				if z.x == b.x and z.y == b.y then
			  sfx(0) 
					del(zombies,z)
					del(bullet,b)
					score+=100
					spawn_zombies(3)	
			end
		end
	end
end 

-- faire apparaitre les zombies
-- aleatoirement
-- et avec un ecart entre eux 

function spawn_zombies(amount)

gap=(128-8*amount)/(amount+1)
 
 for i=1,amount do 
 	 add(zombies,
 	 {x=gap*i + 8*(i-1),
 	 y=ceil(rnd(120)),
 	 sprite=35,
 	 speed=0.25})
			for z in all(zombies) do
 	 	if check_flag(0,ceil(z.x/8),ceil(z.y/8))
				then
	 			del(zombies,z)
 			end
 		end	  
	end
end

-- mouvements des zombies
-- en fonction de michonne 

function update_zombies()
 kill_zemmour()
 for z in all(zombies) do
		-- speed
	if flr(rnd(10)) + 1 == 3 then
	-- collision
			newx = z.x
			newy = z.y	

			if (z.x/8 < player.x) then
				newx = z.x + 1
			end
			if (z.x/8 > player.x) then
				newx = z.x - 1
			end
			if (z.y/8 < player.y) then
			 newy= z.y + 1
			end
			if (z.y/8 > player.y) then 
			 newy= z.y - 1 
	 	end
			if z.y>128 
				then del(zombies,z)
			end
			
			if not check_flag(0,ceil(newx/8),ceil(newy/8))
			then
			-- on avance
				z.x=newx
				z.y=newy
			end
	end	
	end 
end 

function update_zombie()
	if #zombies<1 then 
				spawn_zombies(1)
	end
end





