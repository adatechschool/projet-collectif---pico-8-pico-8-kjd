-- michonne

function create_player()
 player={
 x=6,
 y=3,
 sprite=33,
 life=10,
 speed=1}
end


function draw_player()
	spr(player.sprite,player.x*8,player.y*8)
end

function player_death()
	for z  in all(zombies) do
		if z.x/8 == player.x and z.y/8 == player.y then
			player.life-=1
			del(zombies,z)
			game_over()
		end
	end					
end

function game_over()
	if player.life==0 then
	  sfx(1)
	 _init2()
	 score=0
		end
end


-- gere les mouvement du joueur 

function player_movement()
	newx=player.x
	newy=player.y

	if btnp(➡️)
	then
	newx+=1
	player.sprite=33
	end
	if btnp(⬅️)
	then
	newx-=1
	player.sprite=32
	end
	if btnp(⬇️)
	then
	newy+=1
	player.sprite=34 
	end
	if btnp(⬆️)
 then 
 newy-=1
 player.sprite=49
 end 
	if (btnp(❎)) shoot() 

  
 if not check_flag(0,newx,newy)
	then
	-- on avance
		player.x=newx
		player.y=newy
 end
	-- ne pas sortir de la map
	
	if (player.x<0) player.x=0
	if (player.y<0) player.y=0
	if (player.x>123) player.x=123
	if (player.y>63) player.y=63
end 







