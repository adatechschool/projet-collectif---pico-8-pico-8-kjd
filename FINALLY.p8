function _init()
	scene="menu"
	map(0,0,0,0)
	create_player()
 zombies={}
 bullet={}
 score=0
 if game_over() then 
 _init2()
 end
end

function _init2()
 scene=" re start"
 create_player()
 zombies={}
 bullet={}
 map(0,0,0,0)
		cls()
		rect(4,0,124,40,6)
		spr(192,10,5,4,4)
		spr(140,50,5,4,4)
		spr(204,90,5,4,4)
		print("game over ...",40,50,8)
		print("your score is ",40,60,8)
		print(score,60,70,8)
		print(" try again ?",40 ,80,8)
		print("michonne's zombalad",30,90,8)
		print("press ❎ to restart",30,100,8)
end
	-- update principal game and intro

function _update60()
	 update_menu()
		update_zombie()
		update_zombies(player,zombies)
		player_movement()
		update_bullet()
		player_death()
end 
		

function _draw()
	draw_menu_()
	
end







