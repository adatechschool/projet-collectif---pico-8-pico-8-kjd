 --zombies
 --kjd
 
 function _init()
 map(0,0,0,0)
 x= 60
 y= 60
 
 end

function _update60()

 if (btn(➡️)) x+=1 
 if (btn(⬅️)) x-=1
 if (btn(⬆️)) y-=1
 if (btn(⬇️)) y+=1
 
 
end 

function _draw()

cls()
map(0,0,0,0)

 draw_moves()


end




-->8

-->8
--map

--function draw_map()
-- map(0,0,0,0)
--end

-->8
--obstacle 

function can_move(x, y, w, h)
	if (check_obstacle(x, y)) return false         -- coin haut-gauche
	if (check_obstacle(x+w-1, y)) return false     -- coin haut-droite
	if (check_obstacle(x, y+h-1)) return false     -- coin bas-gauche
	if (check_obstacle(x+w-1, y+h-1)) return false -- coin bas-droite
	return true
end
-->8
-- moves

function draw_moves()


 
 spr(33,x,y)
 
 if (btn(➡️)) spr(46,x,y)
 if (btn(⬅️)) spr(35,x,y)
 if (btn(⬆️)) spr(37,x,y)
 if (btn(⬇️)) spr(14,x,y)
 
 end
 
 
